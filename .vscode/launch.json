  "version": "0.2.0",
  "configurations": [

    {
      "name": "Python: Command",
      "type": "python",
      "request": "launch",
      "program": "__main__.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Command"
    },
    {
      "name": "Python: Observer",
      "type": "python",
      "request": "launch",
      "program": "__main__.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Observer"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
